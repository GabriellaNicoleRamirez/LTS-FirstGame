    if (menu_position = 0){
        if(key_space = 1){
            room_goto(room_movement)
            }
        }
    else if(menu_position = 1){
        if(key_space = 1){
            global.enemy_health = global.enemy_health - global.player_attack
            global.turn++
            }
        }
    }

//turncheck
if(global.turn &gt; global.turn_max){
